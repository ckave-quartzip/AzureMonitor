    {
      "id": "resources",
      "name": "Resource Monitoring",
      "priority": "P0",
      "screens": [
        {
          "id": "resources_list",
          "name": "ResourcesListView",
          "description": "Searchable, filterable list of all monitored resources",
          "components": [
            "SearchBar",
            "FilterChips",
            "ResourceRowView",
            "StatusIndicator",
            "PaginationControl"
          ],
          "viewModel": "ResourcesListViewModel",
          "endpoints": [
            {
              "method": "GET",
              "path": "/resources",
              "queryParams": ["status", "resource_type", "client_id", "environment_id", "limit", "offset"],
              "description": "List resources with filters"
            }
          ]
        },
        {
          "id": "resource_detail",
          "name": "ResourceDetailView",
          "description": "Detailed view of a single resource with metrics",
          "components": [
            "ResourceStatusHero",
            "ResourceMetadataSection",
            "ResponseTimeChart",
            "UptimeStatsGrid",
            "CheckResultsList",
            "AzureResourcePanel",
            "CostTrendChart",
            "MonitoringChecksList"
