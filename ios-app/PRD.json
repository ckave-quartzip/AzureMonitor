            "StatisticsCardsView",
            "ResourceHealthChart",
            "PerformanceTrendChart",
            "RecentChecksListView",
            "CriticalAlertsBar"
          ],
          "viewModel": "DashboardViewModel",
          "endpoints": [
            {
              "method": "GET",
              "path": "/dashboard/summary",
              "description": "Get dashboard statistics"
            },
            {
              "method": "GET",
              "path": "/resources?status=down",
              "description": "Get down resources"
            },
            {
              "method": "GET",
              "path": "/alerts?status=active&severity=critical",
              "description": "Get critical active alerts"
            }
          ]
        }
      ],
      "features": [
        {
          "id": "dashboard_stats",
          "name": "Dashboard Statistics",
          "priority": "P0",
          "acceptance": [
            "Shows total resource count",
            "Shows up/down/degraded counts",
            "Shows active alert counts by severity",
            "Shows average uptime percentage"
          ]
        },
        {
          "id": "health_chart",
          "name": "Resource Health Chart",
          "priority": "P0",
          "acceptance": [
            "Displays pie chart of resource status",
            "Chart animates on load",
            "Tapping segment filters resources"
