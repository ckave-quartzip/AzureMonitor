{
  "project": {
    "name": "QuartzMobile",
    "displayName": "Quartz Azure Monitor",
    "version": "1.0.0",
    "platform": "iOS",
    "minimumVersion": "17.0",
    "language": "Swift",
    "swiftVersion": "6.0",
    "uiFramework": "SwiftUI",
    "architecture": "MVVM"
  },
  "api": {
    "baseUrl": "https://zkqhktsvhazeljnncncr.supabase.co/functions/v1/public-api/v1",
    "authHeader": "X-API-Key",
    "responseFormat": "JSON",
    "rateLimit": {
      "requestsPerHour": 1000,
      "burstPerMinute": 100
    }
  },
  "modules": [
    {
      "id": "auth",
      "name": "Authentication",
      "priority": "P0",
      "screens": [
        {
          "id": "login",
          "name": "LoginView",
          "description": "User authentication screen with email/password and SSO options",
          "components": [
            "EmailTextField",
            "PasswordField",
            "SignInButton",
            "MicrosoftSSOButton",
            "BiometricUnlockButton",
            "ForgotPasswordLink"
          ],
          "viewModel": "LoginViewModel",
          "endpoints": [
            {
              "method": "POST",
              "path": "/auth/login",
              "description": "Email/password authentication"
            }
          ]
        }
      ],
      "features": [
        {
          "id": "email_auth",
          "name": "Email/Password Login",
          "priority": "P0",
          "acceptance": [
            "User can enter email and password",
            "Validation shows errors for invalid input",
            "Successful login navigates to Dashboard",
            "Failed login shows error message"
          ]
        },
        {
          "id": "sso_auth",
          "name": "Microsoft SSO",
          "priority": "P0",
          "acceptance": [
            "User can tap Microsoft sign-in button",
            "OAuth flow opens in system browser",
            "Successful auth returns to app with session"
          ]
        },
        {
          "id": "biometric_auth",
          "name": "Biometric Authentication",
          "priority": "P1",
          "acceptance": [
            "Returning users see biometric prompt",
            "Face ID/Touch ID unlocks the app",
            "Fallback to password if biometric fails"
          ]
        }
      ]
    },
    {
      "id": "dashboard",
      "name": "Dashboard",
      "priority": "P0",
      "screens": [
        {
          "id": "dashboard_main",
          "name": "DashboardView",
          "description": "Main overview dashboard with statistics and charts",
