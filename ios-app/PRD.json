              "method": "GET",
              "path": "/resources/{id}/status",
              "description": "Get current status with metrics"
            },
            {
              "method": "GET",
              "path": "/resources/{id}/uptime",
              "queryParams": ["period"],
              "description": "Get uptime statistics"
            },
            {
              "method": "GET",
              "path": "/monitoring-checks",
              "queryParams": ["resource_id"],
              "description": "Get monitoring checks for resource"
            },
            {
              "method": "GET",
              "path": "/monitoring-checks/{id}/results",
              "description": "Get check result history"
            }
