  "dataModels": [
    {
      "name": "Client",
      "properties": [
        {"name": "id", "type": "UUID", "required": true},
        {"name": "name", "type": "String", "required": true},
        {"name": "status", "type": "ClientStatus", "required": true},
        {"name": "contactEmail", "type": "String?", "required": false},
        {"name": "description", "type": "String?", "required": false},
        {"name": "monthlyHostingFee", "type": "Decimal?", "required": false},
        {"name": "createdAt", "type": "Date", "required": true}
      ]
    },
    {
      "name": "Resource",
      "properties": [
        {"name": "id", "type": "UUID", "required": true},
        {"name": "name", "type": "String", "required": true},
        {"name": "resourceType", "type": "ResourceType", "required": true},
        {"name": "status", "type": "ResourceStatus", "required": true},
        {"name": "lastCheckedAt", "type": "Date?", "required": false},
        {"name": "clientId", "type": "UUID?", "required": false},
        {"name": "environmentId", "type": "UUID?", "required": false},
        {"name": "url", "type": "String?", "required": false},
        {"name": "description", "type": "String?", "required": false}
      ]
    },
    {
      "name": "Alert",
      "properties": [
        {"name": "id", "type": "UUID", "required": true},
        {"name": "title", "type": "String", "required": true},
        {"name": "message", "type": "String", "required": true},
        {"name": "severity", "type": "AlertSeverity", "required": true},
        {"name": "status", "type": "AlertStatus", "required": true},
        {"name": "resourceId", "type": "UUID?", "required": false},
        {"name": "triggeredAt", "type": "Date", "required": true},
        {"name": "acknowledgedAt", "type": "Date?", "required": false},
        {"name": "resolvedAt", "type": "Date?", "required": false}
      ]
    },
    {
      "name": "Incident",
      "properties": [
        {"name": "id", "type": "UUID", "required": true},
        {"name": "title", "type": "String", "required": true},
        {"name": "description", "type": "String?", "required": false},
        {"name": "severity", "type": "AlertSeverity", "required": true},
        {"name": "status", "type": "IncidentStatus", "required": true},
        {"name": "createdAt", "type": "Date", "required": true},
        {"name": "resolvedAt", "type": "Date?", "required": false},
        {"name": "resolutionNotes", "type": "String?", "required": false}
      ]
    },
    {
      "name": "DashboardSummary",
      "properties": [
        {"name": "resources", "type": "ResourceStats", "required": true},
        {"name": "alerts", "type": "AlertStats", "required": true},
        {"name": "uptime", "type": "UptimeStats", "required": true}
      ]
    },
    {
      "name": "AzureTenant",
      "properties": [
        {"name": "id", "type": "UUID", "required": true},
        {"name": "name", "type": "String", "required": true},
        {"name": "tenantId", "type": "String", "required": true},
        {"name": "subscriptionId", "type": "String", "required": true},
        {"name": "isEnabled", "type": "Bool", "required": true}
      ]
    },
    {
      "name": "AzureCostSummary",
      "properties": [
        {"name": "totalCost", "type": "Decimal", "required": true},
        {"name": "currency", "type": "String", "required": true},
        {"name": "period", "type": "DatePeriod", "required": true},
        {"name": "byTenant", "type": "[TenantCost]", "required": true}
      ]
